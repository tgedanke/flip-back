<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.flippost.DAO.Samsung.NewDelivery.Models.SamsungRelatedDocument"
           table = "wwwSamsungRelatedDocument">

        <meta attribute = "class-description">
            contains information about business types from samsung
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="identity"/>
        </id>

        <property name = "Code"
                  column = "Code"
                  type = "string"/>

        <property name = "CodeDescription"
                  column = "CodeDescription"
                  type = "string"/>
    </class>
</hibernate-mapping>