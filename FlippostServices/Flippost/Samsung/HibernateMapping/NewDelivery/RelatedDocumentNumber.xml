<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.flippost.DAO.Samsung.NewDelivery.Models.SamsungRelatedDocumentNumber"
           table = "wwwSamsungRelatedDocumentNumber">

        <meta attribute = "class-description">
            contains information about business types from samsung
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="identity"/>
        </id>

        <property name = "RelatedMessageNumber"
                  column = "RelatedMessageNumber"
                  type = "string"/>

        <property name = "RelatedDocumentNumber"
                  column = "RelatedDocumentNumber"
                  type = "string"/>

        <property name = "RelatedDocumentDate"
                  column = "RelatedDocumentDate"
                  type = "string"/>
    </class>
</hibernate-mapping>