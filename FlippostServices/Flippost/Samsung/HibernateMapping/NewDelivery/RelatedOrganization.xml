<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.flippost.DAO.Samsung.NewDelivery.Models.SamsungRelatedOrganization"
           table = "wwwSamsungRelatedOrganization">

        <meta attribute = "class-description">
            contains information about business types from samsung
        </meta>

        <id name = "id" type = "java.lang.Integer" column = "businessTypeID">
            <generator class="identity"/>
        </id>

        <property name = "Type"
                  column = "Type"
                  type = "string"/>

        <property name = "TypeName"
                  column = "TypeName"
                  type = "string"/>

        <property name = "Name"
                  column = "Name"
                  type = "string"/>

        <property name = "Address1"
                  column = "Address1"
                  type = "string"/>

        <property name = "Address2"
                  column = "Address2"
                  type = "string"/>

        <property name = "Address3"
                  column = "Address3"
                  type = "string"/>

        <property name = "Address4"
                  column = "Address4"
                  type = "string"/>

        <property name = "Address5"
                  column = "Address5"
                  type = "string"/>

        <property name = "Code"
                  column = "Code"
                  type = "string"/>

        <property name = "TelephoneNumber"
                  column = "TelephoneNumber"
                  type = "string"/>

        <property name = "FaxNumber"
                  column = "FaxNumber"
                  type = "string"/>

        <property name = "ContactPerson"
                  column = "ContactPerson"
                  type = "string"/>
    </class>
</hibernate-mapping>